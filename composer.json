{
  "name": "waiterphp/app",
  "description": "waiterphp应用开发框架",
  "type": "project",
  "require": {
    "php": ">=5.4.0",
    "waiterphp/core": ">0.0.0",
    "waiterphp/builder": ">0.0.0",
    "waiterphp/service": ">0.0.0",
    "waiterphp/admin": ">0.0.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Tian Zheng"
    }
  ],
  "autoload": {
    "psr-4": {
      "Behavior\\": "behavior/",
      "Controller\\": "controller/",
      "Model\\": "model/",
      "Router\\": "router/",
      "Service\\": "service/",
      "Tools\\": "tools/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Behavior\\": "tests/behavior/",
      "Tests\\Controller\\": "tests/controller/",
      "Tests\\Model\\": "tests/model/",
      "Tests\\Router\\": "tests/router/",
      "Tests\\Service\\": "tests/service/",
      "Tests\\Tools\\": "tests/tools/"
    }
  }
} 